[gerrit]
	basePath = git
	canonicalWebUrl = https://go.minux.info/
[database]
	type = h2
	database = db/ReviewDB
[index]
	type = LUCENE
[auth]
	type = HTTP
	httpHeader = GITHUB_USER
	logoutUrl = /oauth/reset
[sendemail]
	smtpServer = smtp.gmail.com
	smtpEncryption = TLS
	smtpUser = go.zh.bot@gmail.com
	includeDiff = true
	maximumDiffSize = 262144
[container]
	user = gerrit
	javaHome = /usr/lib/jvm/java-7-openjdk-amd64/jre
[sshd]
	listenAddress = *:29418
[httpd]
	listenUrl = proxy-https://localhost:8000/
	filterClass = com.googlesource.gerrit.plugins.github.oauth.OAuthFilter
[cache]
	directory = cache
[github]
	url = https://github.com
	clientId = XXXXXXXXXXXXXXXXXXXX
	clientSecret = XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
